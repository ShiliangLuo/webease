{
  "name": "webease",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:client": "cross-env RUN_ENV=client vue-cli-service build",
    "build:server": "cross-env RUN_ENV=server vue-cli-service build --mode server",
    "dev": "concurrently -r \"npm run serve\" \"npm run service\"",
    "service": "node server\\index.js",
    "start": "npm run build:server && npm run build:client && npm run service",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "apicache": "^1.6.2",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-plus": "^1.0.2-beta.46",
    "express": "^4.17.1",
    "koa": "^2.13.1",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "md5": "^2.3.0",
    "music-metadata": "^7.8.6",
    "pac-proxy-agent": "^4.1.0",
    "qrcode": "^1.4.4",
    "request": "^2.88.2",
    "store": "^2.0.12",
    "tunnel": "^0.0.6",
    "vue": "^3.1.1",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/store": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "lint-staged": "^9.5.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "memory-fs": "^0.5.0",
    "sass": "^1.34.1",
    "sass-loader": "^10.0.0",
    "typescript": "~4.1.5",
    "vue-jest": "^5.0.0-0",
    "webpack": "^4.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}