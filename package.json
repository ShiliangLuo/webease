{
  "name": "webease",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:client": "cross-env RUN_ENV=client vue-cli-service build",
    "build:server": "cross-env RUN_ENV=server vue-cli-service build --mode server",
    "dev": "concurrently -r \"npm run serve\" \"npm run service\"",
    "service": "node server\\index.js",
    "start": "npm run build:server && npm run build:client && npm run service"
  },
  "dependencies": {
    "apicache": "^1.6.2",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-plus": "^1.0.2-beta.46",
    "express": "^4.17.1",
    "koa": "^2.13.1",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "md5": "^2.3.0",
    "music-metadata": "^7.8.6",
    "pac-proxy-agent": "^4.1.0",
    "qrcode": "^1.4.4",
    "request": "^2.88.2",
    "store": "^2.0.12",
    "tunnel": "^0.0.6",
    "vue": "^3.1.1",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/cli-plugin-babel": "~5.0.0-beta.2",
    "@vue/cli-plugin-router": "~5.0.0-beta.2",
    "@vue/cli-plugin-vuex": "~5.0.0-beta.2",
    "@vue/cli-service": "~5.0.0-beta.2",
    "@vue/compiler-sfc": "^3.1.1",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "memory-fs": "^0.5.0",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "vue-server-renderer": "^2.6.11",
    "webpack": "^5.38.1",
    "webpack-node-externals": "^3.0.0"
  }
}
